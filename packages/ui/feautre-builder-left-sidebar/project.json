{
  "name": "ui-feautre-builder-left-sidebar",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/ui/feautre-builder-left-sidebar/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/ui/feautre-builder-left-sidebar/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/ui/feautre-builder-left-sidebar/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/ui/feautre-builder-left-sidebar/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "tsConfig": "packages/ui/feautre-builder-left-sidebar/tsconfig.spec.json",
        "karmaConfig": "packages/ui/feautre-builder-left-sidebar/karma.conf.js",
        "polyfills": ["zone.js", "zone.js/testing"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/ui/feautre-builder-left-sidebar/**/*.ts",
          "packages/ui/feautre-builder-left-sidebar/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
