{
  "name": "embedding-client",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/ee/embedding-client/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "vendorChunk": false,
        "outputPath": "dist/packages/ee/embedding-client",
        "compiler": "swc",
        "main": "packages/ee/embedding-client/src/main.ts",
        "tsConfig": "packages/ee/embedding-client/tsconfig.app.json",
        "webpackConfig": "packages/ee/embedding-client/webpack.config.js",
        "assets": [
          "packages/ee/embedding-client/src/favicon.ico",
          "packages/ee/embedding-client/src/assets"
        ],
        "index": "packages/ee/embedding-client/src/index.html",
        "baseHref": "/",
        "styles": [
          "packages/ee/embedding-client/src/styles.less"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "packages/ee/embedding-client/src/environments/environment.ts",
              "with": "packages/ee/embedding-client/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "embedding-client:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "embedding-client:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/ee/embedding-client/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/ee/embedding-client/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}