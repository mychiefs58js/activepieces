{
  "name": "ee-embed-sdk-shared",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ee/embed-sdk-shared/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/ee/embed-sdk-shared",
        "main": "packages/ee/embed-sdk-shared/src/index.ts",
        "tsConfig": "packages/ee/embed-sdk-shared/tsconfig.lib.json",
        "assets": [
          "packages/ee/embed-sdk-shared/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/ee/embed-sdk-shared/**/*.ts",
          "packages/ee/embed-sdk-shared/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/ee/embed-sdk-shared/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}