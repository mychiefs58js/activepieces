{
  "name": "ee-guess-flow",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/ee/guess-flow/src",
  "prefix": "activepieces",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/ee/guess-flow/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/ee/guess-flow/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/ee/guess-flow/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "tsConfig": "packages/ee/guess-flow/tsconfig.spec.json",
        "karmaConfig": "packages/ee/guess-flow/karma.conf.js",
        "polyfills": ["zone.js", "zone.js/testing"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/ee/guess-flow/**/*.ts",
          "packages/ee/guess-flow/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
