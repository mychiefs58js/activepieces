{
  "name": "piece-builder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build && mv dist/piece-builder _static",
    "build:stg": "ng build --configuration=staging && mv dist/piece-builder _static",
    "build:prod": "ng build --configuration=production && mv dist/piece-builder _static",
    "build:review": "ng build --configuration=staging",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "ngcc",
    "lint": "ng lint",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "prepare": "",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:check"
    }
  },
  "private": true,
